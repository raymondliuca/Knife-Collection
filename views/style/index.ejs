<h1><%= style.name %></h1>

<div class="container">
    <div class="row">
        <% knifes.forEach(function(knife) { %>
            <div class="col">
                <div class="img">
                    <img calss="img_size" src="<%= knife.picture %>" >
                </div>
            </div>
            <div class="name">
                <a href="/knife/:id=<%= knife.id %>">
                    <%= knife.length %>
                </a>
            </div>
        <% }) %>
    </div>
</div> 
